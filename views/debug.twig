{# shared-media-gallery  admin.twig  v0.0.1 #}
{{ include('header.twig') }}
{{ include('menu.twig') }}
<div class="content">
	[ADMIN][DEBUG]
<pre>
uriBase    : {{ uriBase }}
uriRelative: {{ uriRelative }}
uri        : {{ uri|join(', ') }}
uri.0      : {{ uri.0 is not null ? uri.0 : '-' }}
uri.1      : {{ uri.1 is not null ? uri.1 : '-' }}
uri.2      : {{ uri.2 is not null ? uri.2 : '-' }}
uri.3      : {{ uri.3 is not null ? uri.3 : '-' }}
{% for view, route in routes %}
{{ view }}:
	route: {{ route|join(', ') }}
	route.0: {{ route.0 is not null ? route.0 : '-' }}
	route.1: {{ route.1 is not null ? route.1 : '-' }}
	route.2: {{ route.2 is not null ? route.2 : '-' }}
	route.3: {{ route.3 is not null ? route.3 : '-' }}
{% endfor %}

</pre>
</div>
{{ include('footer.twig') }}
